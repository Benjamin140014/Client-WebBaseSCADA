        <!-- Connect to opc server-->
        <div class="connect-container">
          <div
          class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
          <h1 class="h2" id="header_connect" style="font-weight: bold;" >Connect</h1>
          </div>
          <div class="card bg-light text-dark">
            <div class="card-header" style="display: flex;">
            <h3 style="margin-right: 20px;">Server Address :</h3>
            <h3 id="endpoint">opc.tcp://</h3>
            </div>
            <div class="card-body">
              <form class="form" role="form" autocomplete="off">
                <div class="form-group row">
                    <label class="col-lg-6 col-form-label form-control-label">Address</label>
                    <div class="col-lg-6">
                        <input class="form-control"
                         type="text" 
                         id="addressOfServer"
                         placeholder="Address name"
                         onkeypress="changeAddress()"/>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-lg-6 col-form-label form-control-label">Port</label>
                    <div class="col-lg-6">
                        <input class="form-control" 
                        type="text" 
                        id="portOfServer"
                        placeholder="Port name"
                        onkeypress="changePort()"/>
                    </div>
                </div>
                <div class="form-group row">
                  <label class="col-lg-6 col-form-label form-control-label">Path(optional)</label>
                  <div class="col-lg-6">
                      <input class="form-control" 
                      type="text" 
                      id="pathOfServer"
                      placeholder="Path name"
                      onkeypress="changePath()"/>
                  </div>
              </div>
                <div class="form-group row">
                    <label class="col-lg-2 col-form-label form-control-label">Security Modes</label>
                    <div class="col-lg-3">
                      <input
                      type="checkbox"
                      name="None"
                      id="securityModeNone"
                      value="None"
                      onclick="disable1()"
                    />
                    <label for="None">None</label>
                    </div>
                    <div class="col-lg-3">
                      <input
                      type="checkbox"
                      name="securityModeSign"
                      id="securityModeSign"
                      value="Sign"
                      onclick="disable2()"
                    />
                   <label for="securityModeSign">Sign</label>
                    </div>
                    <div class="col-lg-3">
                      <input
                      type="checkbox"
                      name="securityModeSignEncrypt"
                      id="securityModeSignEncrypt"
                      value="SignEncypt"
                      onclick="disable3()"
                    />
                    <label for="securityModeSignEncrypt"> Sign&Encrypt</label>
                    </div>
                </div>
  
                <div class="form-group row">
                  <label class="col-lg-2 col-form-label form-control-label">Security Policies</label>
                  <div class="col-lg-2">
                    <input
                    type="checkbox"
                    name="None"
                    id="securityPoliciesNone" 
                    value="None"
                    onclick="disable4()"
                    />
                    <label for="None"> None</label>
                  </div>
                  <div class="col-lg-3">
                    <input
                    type="checkbox"
                    name="Basic128Rsa15"
                    id="securityPoliciesBasic128Rsa15"
                    value="Basic128Rsa15"
                    onclick="disable5()"
                   />
                   <label for="Basic128Rsa15"> Basic128Rsa15</label>
                  </div>
                  <div class="col-lg-2">
                    <input
                    type="checkbox"
                    name="Basic256"
                    id="securityPoliciesBasic256"
                    value="Basic256"
                    onclick="disable6()"
                  />
                  <label for="Basic256"> Basic256</label>
                  </div>
                  <div class="col-lg-3">
                    <input
                    type="checkbox"
                    name="Basic256Sha256"
                    id="securityPoliciesBasic256Sha256"
                    value="Basic256Sha256"
                    onclick="disable7()"
                    />
                    <label for="Basic256Sha256"> Basic256Sha256</label>
                  </div>
                </div>
  
                <div class="form-group row">
                  <label class="col-lg-3 col-form-label form-control-label">Authentication</label>
                  <div class="col-lg-3">
                    <input
                    type="checkbox"
                    name="anonymous"
                    id='anonymous'
                    value="anonymous"
                    onclick="AuthenticationSetting()"
                  />
                  <label for="anonymous"> Anonymous </label> 
                  </div>
                  <div class="col-lg-6">
                    <input
                    type="checkbox"
                    name="user"
                    id="authenticationUser"
                    value="user"
                    onclick="DisableAnonymous()"
                  />
                  <label for="user">user</label> 
                  </div>
                </div>
  
               <div class="form-group row">
                <label class="col-lg-6 col-form-label form-control-label"></label>
                <label class="col-lg-2 col-form-label form-control-label">Username</label>
                <div class="col-lg-4">
                  <input class="form-control" 
                  type="text" 
                  placeholder="Username" 
                  name="username" 
                  id="usernameUser" />
                </div>
               </div>
  
               <div class="form-group row">
                <label class="col-lg-6 col-form-label form-control-label"></label>
                <label class="col-lg-2 col-form-label form-control-label">Password</label>
                <div class="col-lg-4">
                  <input class="form-control" 
                  type="password" 
                  placeholder="Password"
                  name="password" 
                  id="passwordUser" />
                </div>
               </div>
  
                <div class="form-group row">
                 <div class="col lg-1">
                  <h5> Status : </h5>
                 </div>
                 <div class="col lg-1">
                 <h5 id="statusClient" style="color: red;" ><i class="fas fa-times"> </i> No Connection</h5>
                 </div>
                 <div class="col lg-4">
                 <h6 id = "messageStatusClient"></h6>
                 </div>
                <div class="col-lg-2">
                  <button type="button" class="btn btn-primary" id="btnConnect" onclick="applyConnect()"> Connect </button>
                </div>
                <div class="col-lg-2">
                <h5 id="messageApplyServer"></h5>
                </div>
                <div class="col-lg-2">
                 <button type="button" class="btn btn-danger" id="btnDisconnect" onclick="Disconnect()"> Disconnect</button>
                </div>
                </div>
            </form>
